        <div class="tab-pane fade" id="nav-contact" role="tabpanel" aria-labelledby="nav-contact-tab">
            <div class="row">
                <% for(let i=0;i < data.length;i++) { %>
                <div class="col-sm-4">
                  <div class=" card shadow p-3 mb-5 bg-white rounded" >
                    <div class="card-body">
                        <div class="jumbotron jumbotron-fluid"  style=" background-color:#B3E8FF;border-radius: 5px ;" >
                            <div class="container">
                              <h1 class="display-4"><%= data[i].projectName%></h1>
                              <p class="lead"><%= data[i].aboutProject %></p>
                            </div>
                          </div>
                      <form>
                        <div class="form-group">
                          <label for="selectEmployee">Select a Employee</label>
                          <select class="form-control" id="selectedEmployee">
                            <option selected>rahul</option>
                            <option>sachin</option>
                            <option>sandeep sir</option>
                            <option>suraj</option>
                          </select>
                        </div>
                        </form>
                      <% var url = data[i].projectName %>
                      <a href=<%="/issues/" + url.replace(/\s/g,"|") %> class="btn btn-primary">Associate</a>
                    </div>
                  </div>
                </div>
